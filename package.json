{
  "name": "coding-interview-backend-level-3",
  "version": "1.0.0",
  "description": "Coding Interview Backend Level 3",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "npm run db:migrate && nodemon --watch \"./**/*.ts\" --exec \"npx tsx\" src/index.ts",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/config.js --migrations-path src/db/migrations",
    "db:g:migration": "npx sequelize-cli model:generate --name",
    "db:migrate:down": "npx sequelize-cli db:migrate:undo --migrations-path src/dc/migrations",
    "db:create": "npx sequelize-cli db:create",
    "db:g:seed": "npx sequelize-cli seed:generate --name",
    "db:run:seed": "npx sequelize-cli db:seed:all"
  },
  "author": "El Dorado Dev Team",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.7.2",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/pg": "^8.11.10",
    "jest": "^29.7.0",
    "mocha": "^10.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.10",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.0",
    "uuid": "^9.0.0"
  }
}
